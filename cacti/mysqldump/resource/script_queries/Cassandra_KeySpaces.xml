	<interface>
		<name>X Cassandra KeySpaces DQ</name>
		<description>Gets a list of KeySpaces</description>
		<script_path>php |path_cacti|/scripts/ss_get_cassandra_stats.php</script_path>
		<arg_prepend>--host |host_hostname|</arg_prepend>
		<arg_index>--ksindex</arg_index>
		<arg_query>--query</arg_query>
		<arg_get>--get</arg_get>
		<output_delimeter>:</output_delimeter>
		<index_order>cassKeyspace</index_order>
		<index_order_type>alphabetic</index_order_type>
		<index_title_format>|chosen_order_field|</index_title_format>
		<fields>
			<cassKeyspace>
				<name>CassKeyspace</name>
				<direction>input</direction>
				<query_name>cassKeyspace</query_name>
			</cassKeyspace>
			<cassKSKeyCacheSize>
				<name>CassKSKeyCacheSize</name>
				<direction>output</direction>
				<query_name>cassKSKeyCacheSize</query_name>
			</cassKSKeyCacheSize>
			<cassKSKeyCacheCapacity>
				<name>CassKSKeyCacheCapacity</name>
				<direction>output</direction>
				<query_name>cassKSKeyCacheCapacity</query_name>
			</cassKSKeyCacheCapacity>
			<cassKSRowCacheSize>
				<name>CassKSRowCacheSize</name>
				<direction>output</direction>
				<query_name>cassKSRowCacheSize</query_name>
			</cassKSRowCacheSize>
			<cassKSRowCacheCapacity>
				<name>CassKSRowCacheCapacity</name>
				<direction>output</direction>
				<query_name>cassKSRowCacheCapacity</query_name>
			</cassKSRowCacheCapacity>
			<cassKSKeyCacheRequests>
				<name>CassKSKeyCacheRequests</name>
				<direction>output</direction>
				<query_name>cassKSKeyCacheRequests</query_name>
			</cassKSKeyCacheRequests>
			<cassKSKeyCacheHits>
				<name>CassKSKeyCacheHits</name>
				<direction>output</direction>
				<query_name>cassKSKeyCacheHits</query_name>
			</cassKSKeyCacheHits>
			<cassKSRowCacheRequests>
				<name>CassKSRowCacheRequests</name>
				<direction>output</direction>
				<query_name>cassKSRowCacheRequests</query_name>
			</cassKSRowCacheRequests>
			<cassKSRowCacheHits>
				<name>CassKSRowCacheHits</name>
				<direction>output</direction>
				<query_name>cassKSRowCacheHits</query_name>
			</cassKSRowCacheHits>
			<cassKSReadCount>
				<name>CassKSReadCount</name>
				<direction>output</direction>
				<query_name>cassKSReadCount</query_name>
			</cassKSReadCount>
			<cassKSWriteCount>
				<name>CassKSWriteCount</name>
				<direction>output</direction>
				<query_name>cassKSWriteCount</query_name>
			</cassKSWriteCount>
			<cassKSTotalReadLatencyMicros>
				<name>CassKSTotalReadLatencyMicros</name>
				<direction>output</direction>
				<query_name>cassKSTotalReadLatencyMicros</query_name>
			</cassKSTotalReadLatencyMicros>
			<cassKSTotalWriteLatencyMicros>
				<name>CassKSTotalWriteLatencyMicros</name>
				<direction>output</direction>
				<query_name>cassKSTotalWriteLatencyMicros</query_name>
			</cassKSTotalWriteLatencyMicros>
			<cassKSTotalDiskSpaceUsed>
				<name>CassKSTotalDiskSpaceUsed</name>
				<direction>output</direction>
				<query_name>cassKSTotalDiskSpaceUsed</query_name>
			</cassKSTotalDiskSpaceUsed>
			<cassKSLiveDiskSpaceUsed>
				<name>CassKSLiveDiskSpaceUsed</name>
				<direction>output</direction>
				<query_name>cassKSLiveDiskSpaceUsed</query_name>
			</cassKSLiveDiskSpaceUsed>
			<cassKSBloomFilterFalsePositives>
				<name>CassKSBloomFilterFalsePositives</name>
				<direction>output</direction>
				<query_name>cassKSBloomFilterFalsePositives</query_name>
			</cassKSBloomFilterFalsePositives>
			<cassKSLiveSSTableCount>
				<name>CassKSLiveSSTableCount</name>
				<direction>output</direction>
				<query_name>cassKSLiveSSTableCount</query_name>
			</cassKSLiveSSTableCount>
			<cassKSPendingTasks>
				<name>CassKSPendingTasks</name>
				<direction>output</direction>
				<query_name>cassKSPendingTasks</query_name>
			</cassKSPendingTasks>
			<cassKSMeanRowCompactedSize>
				<name>CassKSMeanRowCompactedSize</name>
				<direction>output</direction>
				<query_name>cassKSMeanRowCompactedSize</query_name>
			</cassKSMeanRowCompactedSize>
			<cassKSMemtableSwitchCount>
				<name>CassKSMemtableSwitchCount</name>
				<direction>output</direction>
				<query_name>cassKSMemtableSwitchCount</query_name>
			</cassKSMemtableSwitchCount>
			<cassKSMemtableDataSize>
				<name>CassKSMemtableDataSize</name>
				<direction>output</direction>
				<query_name>cassKSMemtableDataSize</query_name>
			</cassKSMemtableDataSize>
			<cassKSMemtableColumnsCount>
				<name>CassKSMemtableColumnsCount</name>
				<direction>output</direction>
				<query_name>cassKSMemtableColumnsCount</query_name>
			</cassKSMemtableColumnsCount>
		</fields>
	</interface>
