	<interface>
		<name>X Cassandra ColumnFamilies DQ</name>
		<description>Gets a list of Column_Families</description>
		<script_path>php |path_cacti|/scripts/ss_get_cassandra_stats.php</script_path>
		<arg_prepend>--host |host_hostname|</arg_prepend>
		<arg_index>--cfindex</arg_index>
		<arg_query>--query</arg_query>
		<arg_get>--get</arg_get>
		<output_delimeter>:</output_delimeter>
		<index_order>cassColumnFamily</index_order>
		<index_order_type>alphabetic</index_order_type>
		<index_title_format>|chosen_order_field|</index_title_format>
		<fields>
			<cassColumnFamily>
				<name>CassColumnFamily</name>
				<direction>input</direction>
				<query_name>cassColumnFamily</query_name>
			</cassColumnFamily>
			<cassCFKeyCacheSize>
				<name>CassCFKeyCacheSize</name>
				<direction>output</direction>
				<query_name>cassCFKeyCacheSize</query_name>
			</cassCFKeyCacheSize>
			<cassCFKeyCacheCapacity>
				<name>CassCFKeyCacheCapacity</name>
				<direction>output</direction>
				<query_name>cassCFKeyCacheCapacity</query_name>
			</cassCFKeyCacheCapacity>
			<cassCFRowCacheSize>
				<name>CassCFRowCacheSize</name>
				<direction>output</direction>
				<query_name>cassCFRowCacheSize</query_name>
			</cassCFRowCacheSize>
			<cassCFRowCacheCapacity>
				<name>CassCFRowCacheCapacity</name>
				<direction>output</direction>
				<query_name>cassCFRowCacheCapacity</query_name>
			</cassCFRowCacheCapacity>
			<cassCFKeyCacheRequests>
				<name>CassCFKeyCacheRequests</name>
				<direction>output</direction>
				<query_name>cassCFKeyCacheRequests</query_name>
			</cassCFKeyCacheRequests>
			<cassCFKeyCacheHits>
				<name>CassCFKeyCacheHits</name>
				<direction>output</direction>
				<query_name>cassCFKeyCacheHits</query_name>
			</cassCFKeyCacheHits>
			<cassCFRowCacheRequests>
				<name>CassCFRowCacheRequests</name>
				<direction>output</direction>
				<query_name>cassCFRowCacheRequests</query_name>
			</cassCFRowCacheRequests>
			<cassCFRowCacheHits>
				<name>CassCFRowCacheHits</name>
				<direction>output</direction>
				<query_name>cassCFRowCacheHits</query_name>
			</cassCFRowCacheHits>
			<cassCFReadCount>
				<name>CassCFReadCount</name>
				<direction>output</direction>
				<query_name>cassCFReadCount</query_name>
			</cassCFReadCount>
			<cassCFWriteCount>
				<name>CassCFWriteCount</name>
				<direction>output</direction>
				<query_name>cassCFWriteCount</query_name>
			</cassCFWriteCount>
			<cassCFTotalReadLatencyMicros>
				<name>CassCFTotalReadLatencyMicros</name>
				<direction>output</direction>
				<query_name>cassCFTotalReadLatencyMicros</query_name>
			</cassCFTotalReadLatencyMicros>
			<cassCFTotalWriteLatencyMicros>
				<name>CassCFTotalWriteLatencyMicros</name>
				<direction>output</direction>
				<query_name>cassCFTotalWriteLatencyMicros</query_name>
			</cassCFTotalWriteLatencyMicros>
			<cassCFTotalDiskSpaceUsed>
				<name>CassCFTotalDiskSpaceUsed</name>
				<direction>output</direction>
				<query_name>cassCFTotalDiskSpaceUsed</query_name>
			</cassCFTotalDiskSpaceUsed>
			<cassCFLiveDiskSpaceUsed>
				<name>CassCFLiveDiskSpaceUsed</name>
				<direction>output</direction>
				<query_name>cassCFLiveDiskSpaceUsed</query_name>
			</cassCFLiveDiskSpaceUsed>
			<cassCFBloomFilterFalsePositives>
				<name>CassCFBloomFilterFalsePositives</name>
				<direction>output</direction>
				<query_name>cassCFBloomFilterFalsePositives</query_name>
			</cassCFBloomFilterFalsePositives>
			<cassCFLiveSSTableCount>
				<name>CassCFLiveSSTableCount</name>
				<direction>output</direction>
				<query_name>cassCFLiveSSTableCount</query_name>
			</cassCFLiveSSTableCount>
			<cassCFPendingTasks>
				<name>CassCFPendingTasks</name>
				<direction>output</direction>
				<query_name>cassCFPendingTasks</query_name>
			</cassCFPendingTasks>
			<cassCFMeanRowCompactedSize>
				<name>CassCFMeanRowCompactedSize</name>
				<direction>output</direction>
				<query_name>cassCFMeanRowCompactedSize</query_name>
			</cassCFMeanRowCompactedSize>
			<cassCFMemtableSwitchCount>
				<name>CassCFMemtableSwitchCount</name>
				<direction>output</direction>
				<query_name>cassCFMemtableSwitchCount</query_name>
			</cassCFMemtableSwitchCount>
			<cassCFMemtableDataSize>
				<name>CassCFMemtableDataSize</name>
				<direction>output</direction>
				<query_name>cassCFMemtableDataSize</query_name>
			</cassCFMemtableDataSize>
			<cassCFMemtableColumnsCount>
				<name>CassCFMemtableColumnsCount</name>
				<direction>output</direction>
				<query_name>cassCFMemtableColumnsCount</query_name>
			</cassCFMemtableColumnsCount>
		</fields>
	</interface>
